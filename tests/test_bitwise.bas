10 REM TEST BITWISE FUNCTIONS
20 PRINT "--- START BITWISE TEST ---"
30 REM TEST AND
40 A$ = "A" 
50 B$ = "C" 
60 C$ = AND(A$, B$)
70 PRINT "AND('A', 'C') = '"; C$; "' (EXPECTED 'A')"
80 IF C$ = "A" THEN PRINT "PASS: AND" ELSE PRINT "FAIL: AND"
90 REM TEST OR
100 D$ = "A"
110 E$ = "B"
120 F$ = OR(D$, E$)
130 PRINT "OR('A', 'B') = '"; F$; "' (EXPECTED 'C')"
140 IF F$ = "C" THEN PRINT "PASS: OR" ELSE PRINT "FAIL: OR"
150 REM TEST XOR
160 G$ = XOR("A", "C")
170 V1 = ASC(G$)
180 PRINT "XOR('A', 'C') ASC = "; V1; " (EXPECTED 2)"
190 IF V1 = 2 THEN PRINT "PASS: XOR" ELSE PRINT "FAIL: XOR"
200 REM TEST NOT
210 H$ = CHR$(0)
220 I$ = NOT(H$)
230 V2 = ASC(I$)
240 PRINT "NOT(CHR$(0)) ASC = "; V2; " (EXPECTED 255)"
250 IF V2 = 255 THEN PRINT "PASS: NOT" ELSE PRINT "FAIL: NOT"
260 PRINT "--- END BITWISE TEST ---"
270 END
