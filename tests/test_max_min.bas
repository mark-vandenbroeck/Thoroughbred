10 REM Test MAX and MIN
20 LET A = 10
30 LET B = 20
40 PRINT "MAX(10, 20) = ", MAX(A, B)
50 PRINT "MAX(20, 10) = ", MAX(B, A)
60 PRINT "MIN(10, 20) = ", MIN(A, B)
70 PRINT "MIN(20, 10) = ", MIN(B, A)
80 PRINT "MAX(1, 5, 3, 9) = ", MAX(1, 5, 3, 9)
90 PRINT "MIN(1, -5, 3) = ", MIN(1, -5, 3)
100 PRINT "MAX('A', 'B') = ", MAX("A", "B")
110 PRINT "MIN('ANT', 'ZANT') = ", MIN("ANT", "ZANT")
120 END
